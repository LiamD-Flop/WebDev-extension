{
    "manifest_version": 2,
    "name": "Responsive dev",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "main/index.html",
        "main/main.css",
        "main/assets/lock.svg",
        "main/devices.min.css"
    ]
}